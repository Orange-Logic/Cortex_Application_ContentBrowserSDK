  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "npm run build:library && npm run build:types",
    "build:library": "craco build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir build",
    "test": "craco test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "eslint --fix .",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "cypress:test": "cypress run --component --browser chrome"
